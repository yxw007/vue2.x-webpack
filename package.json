    "core-js": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime-corejs3": "^7.17.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-vue": "^2.0.2",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^5.2.0",
    "css-loader": "^6.6.0",
